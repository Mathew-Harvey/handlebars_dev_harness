            bottom: 0;
            width: 100%;
            padding-bottom: 20px;
            align-items: center;
        }

        td,
        th {
            text-align: left;
            border: 1px solid black;
            border: 1px solid black;
            line-height: 50%;
        }

        .noBorder {
            border: none !important;
        }

        .noBorderTopBottom {
            border-top: none !important;
            border-bottom: none !important;
        }

        .noBorderSides {
            border-left: none !important;
            border-right: none !important;
        }

        .noBorderRight {
            border-right: none !important;
        }

        .smallText {
            font-size: 12px;
        }

        .signatureBox {
            padding-bottom: 70px;
        }

        #grey {
            background-color: #dddddd;
        }
    </style>
