        .centeredText {
            text-align: center !important;
        }



        #container {
            -mz-column-count: 2;
            -webkit-column-count: 2;
            column-count: 2;
            height: 60px;
            width: 40px;
        }

        .block {
            width: 40px;
            height: 40px;
            margin: 0 0 5px 0;
        }

        #grid {
            height: 100px;
            display: grid;
            grid-template-areas: "a a";
            gap: 10px;
            grid-template: repeat(2 1fr);
            grid-auto-columns: 90px;
            grid-auto-flow: column;
        }

 

